<MudNavMenu Class="mud-width-full">
    <MudNavGroup Title="Editors" Expanded="true">
        <MudNavLink draggable="true" Match="NavLinkMatch.All"  Icon="@Icons.Filled.TextFields" 
            @ondragstart="@(() => OnDragStart("PoC03B.Client.Shared.Editors.TextBoxComponent"))">
            TextBox
        </MudNavLink>
        <MudNavLink draggable="true" Match="NavLinkMatch.All" Icon="@Icons.Filled.SelectAll" 
            @ondragstart="@(() => OnDragStart("PoC03B.Client.Shared.Editors.SelectComponent"))">
            Select
        </MudNavLink>
        <MudNavLink draggable="true" Match="NavLinkMatch.All" Icon="@Icons.Filled.RadioButtonChecked" 
            @ondragstart="@(() => OnDragStart("PoC03B.Client.Shared.Editors.RadioComponent"))">
            Radio
        </MudNavLink>
        <MudNavLink draggable="true" Match="NavLinkMatch.All" Icon="@Icons.Filled.Checklist" 
            @ondragstart="@(() => OnDragStart("PoC03B.Client.Shared.Editors.CheckBoxComponent"))">
            Check
        </MudNavLink>
        <MudNavLink draggable="true" Match="NavLinkMatch.All" Icon="@Icons.Filled.SwitchLeft" 
            @ondragstart="@(() => OnDragStart("PoC03B.Client.Shared.Editors.SwitchComponent"))">
            Switch
        </MudNavLink>
        <MudNavLink draggable="true" Match="NavLinkMatch.All" Icon="@Icons.Filled.SmartButton" 
            @ondragstart="@(() => OnDragStart("PoC03B.Client.Shared.Editors.ButtonComponent"))">
            Button
        </MudNavLink>
    </MudNavGroup>
</MudNavMenu>

@code {
    [CascadingParameter(Name = "FormDesignerData")] protected FormDesignerModel FormDesignerData { get; set; }

    private void OnDragStart(string typeName)
    {
        FormDesignerData.DragByTypeName = typeName;
    }
}